<?xml version="1.0" encoding="UTF-8"?>
    <swes:InsertSensor service="SOS" version="2.0.0"
      xmlns:swes="http://www.opengis.net/swes/2.0"
      xmlns:sos="http://www.opengis.net/sos/2.0"
      xmlns:swe="http://www.opengis.net/swe/2.0"
      xmlns:sml="http://www.opengis.net/sensorml/2.0"
      xmlns:gml="http://www.opengis.net/gml/3.2"
      xmlns:xlink="http://www.w3.org/1999/xlink"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:gco="http://www.isotc211.org/2005/gco"
      xmlns:gmd="http://www.isotc211.org/2005/gmd" xsi:schemaLocation="http://www.opengis.net/sos/2.0 http://schemas.opengis.net/sos/2.0/sosInsertSensor.xsd   http://www.opengis.net/swes/2.0 http://schemas.opengis.net/swes/2.0/swes.xsd">
      <swes:procedureDescriptionFormat>http://www.opengis.net/sensorml/2.0</swes:procedureDescriptionFormat>
        <swes:procedureDescription>

          <sml:PhysicalSystem gml:id=@@xml document ID, unique system wide@@>
            <gml:description>@@long form text sensor description@@</gml:description>
            <gml:identifier codeSpace="uniqueID">@@system wide unique sensor URI/handle@@</gml:identifier>
            <gml:name>@@sensor text name@@</gml:name>

            <sml:keywords>
              <sml:KeywordList>
                <sml:keyword>@@list of any keywords@@</sml:keyword>
                <sml:keyword>@@list of any keywords@@</sml:keyword>
              </sml:KeywordList>
            </sml:keywords>

            <sml:identification>
              <sml:IdentifierList>
                <sml:identifier>
                  <sml:Term definition="urn:ogc:def:identifier:OGC:1.0:longname">
                    <sml:label>Long Name</sml:label>
                    <sml:value>@@sensor long text name@@</sml:value>
                  </sml:Term>
                </sml:identifier>
                <sml:identifier>
                  <sml:Term definition="urn:ogc:def:identifier:OGC:1.0:shortname">
                    <sml:label>Short Name</sml:label>
                    <sml:value>@@sensor short text name (overlap with gml:name)@@</sml:value>
                  </sml:Term>
                </sml:identifier>
              </sml:IdentifierList>
            </sml:identification>

            <sml:classification>
              <sml:ClassifierList>
                <sml:classifier>
                  <sml:Term definition="http://sensorml.com/ont/swe/property/SensorType">
                    <sml:label>Sensor Type</sml:label>
                    <sml:value>@@vocabulary URI for sensor type@@</sml:value>
                  </sml:Term>
                </sml:classifier>
              </sml:ClassifierList>
            </sml:classification>

            <sml:capabilities name="offerings">
              <sml:CapabilityList>
                <sml:capability name="offeringID">
                  <swe:Text definition="urn:ogc:def:identifier:OGC:offeringID">
                    <swe:label>offeringID</swe:label>
                    <swe:value>@@arbitrary group id for sensors observations unique per sensor - e.g. the gml:identifer with  "/offering" postfix@@</swe:value>
                  </swe:Text>
                </sml:capability>
              </sml:CapabilityList>
            </sml:capabilities>

            <sml:contacts>
              <sml:ContactList>
                <sml:contact xlink:title="pointOfContact" xlink:arcrole="http://inspire.ec.europa.eu/metadata-codelist/ResponsiblePartyRole/pointOfContact">
                  <gmd:CI_ResponsibleParty>
                    <gmd:organisationName>
                      <gco:CharacterString>@@as appropriate@@</gco:CharacterString>
                    </gmd:organisationName>
                    <gmd:contactInfo>
                      <gmd:CI_Contact>
                        <gmd:phone>
                          <gmd:CI_Telephone>
                            <gmd:voice>
                              <gco:CharacterString>@@as appropriate@@</gco:CharacterString>
                            </gmd:voice>                                        
                          </gmd:CI_Telephone>
                        </gmd:phone>
                        <gmd:address>
                          <gmd:CI_Address>
                            <gmd:city>
                              <gco:CharacterString>@@as appropriate@@</gco:CharacterString>
                            </gmd:city>
                            <gmd:postalCode>
                              <gco:CharacterString>@@as appropriate@@</gco:CharacterString>
                            </gmd:postalCode>
                            <gmd:country>
                              <gco:CharacterString>@@as appropriate@@</gco:CharacterString>
                            </gmd:country>
                            <gmd:electronicMailAddress>
                              <gco:CharacterString>@@as appropriate@@</gco:CharacterString>
                            </gmd:electronicMailAddress>
                          </gmd:CI_Address>
                        </gmd:address>
                        <gmd:onlineResource>
                          <gmd:CI_OnlineResource>
                            <gmd:linkage>
                              <gmd:URL>
                                @@as appropriate@@
                              </gmd:URL>
                            </gmd:linkage>
                          </gmd:CI_OnlineResource>
                        </gmd:onlineResource>
                      </gmd:CI_Contact>
                    </gmd:contactInfo>
                    <gmd:role>
                      <gmd:CI_RoleCode 
                        codeList="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/resources/Codelist/gmxCodelists.xml#CI_RoleCode" 
                        codeListValue="pointOfContact">
                      </gmd:CI_RoleCode>
                    </gmd:role>
                  </gmd:CI_ResponsibleParty>
                </sml:contact>
              </sml:ContactList>
            </sml:contacts>
            
            <sml:featuresOfInterest>
              <sml:FeatureList definition="http://www.opengis.net/def/featureOfInterest/identifier">
                <sml:feature xlink:href="@@WFS URI of domain|sampled feature@@"
                  xlink:title="@@featureID@@">
                  <swe:label>@@as appropriate@@</swe:label>
                </sml:feature>
              </sml:FeatureList>
            </sml:featuresOfInterest>
            
            <sml:inputs>
              <sml:InputList>
                <sml:input name="@@input phenomena xml document id@@">
                  <sml:ObservableProperty definition="@@vocabulary uri@@">
                    <swe:label>@@text label value@@</swe:label>
                  </sml:ObservableProperty>
                </sml:input>
              </sml:InputList>    
            </sml:inputs>
            
            <sml:outputs>
              <sml:OutputList>
                <sml:output name="@@output phenomena xml document id@@">
                  <swe:Quantity definition="@@vocabulary uri@@">
                    <swe:label>@@text label value@@</swe:label>
                    <swe:uom code="@@as-appropriate@@"/>
                  </swe:Quantity>
                </sml:output>
              </sml:OutputList>
            </sml:outputs>
            
            <sml:position>
              <swe:Vector referenceFrame="http://www.opengis.net/def/crs/EPSG/0/4326">
                <swe:coordinate name="northing">
                  <swe:Quantity definition="latitude" axisID="Lat">
                    <swe:uom xlink:href="http://vocabs.lter-europe.net/EnvThes/EUUnits_4" code="degree"/>
                    <swe:value>@@as-appropriate@@</swe:value>
                  </swe:Quantity>
                </swe:coordinate>
                <swe:coordinate name="easting">
                  <swe:Quantity definition="longitude" axisID="Lon">
                    <swe:uom xlink:href="http://vocabs.lter-europe.net/EnvThes/EUUnits_4" code="degree"/>
                    <swe:value>@@as-appropriate@@</swe:value>
                  </swe:Quantity>
                </swe:coordinate>
                <swe:coordinate name="altitude">
                  <swe:Quantity definition="altitude" axisID="Alt">
                    <swe:uom xlink:href="http://vocabs.lter-europe.net/EnvThes/EUUnits_110" code="m"/>
                    <swe:value>@@as-appropriate@@</swe:value>
                  </swe:Quantity>
                </swe:coordinate>
              </swe:Vector>
            </sml:position>
            
            </sml:PhysicalSystem>
          </swes:procedureDescription>
          <!-- multiple values possible -->
          <swes:observableProperty>@@Vocabulary URI of phenomena being observed, must match the result template entry@@</swes:observableProperty>   
          <swes:metadata>
            <sos:SosInsertionMetadata>
              <sos:observationType>http://www.opengis.net/def/observationType/OGC-OM/2.0/OM_Measurement</sos:observationType>
              <!-- multiple values possible -->
              <sos:featureOfInterestType>http://www.opengis.net/def/samplingFeatureType/OGC-OM/2.0/SF_SamplingPoint</sos:featureOfInterestType>
            </sos:SosInsertionMetadata>
          </swes:metadata>
        </swes:InsertSensor>


<?xml version="1.0" encoding="UTF-8"?>
  <sos:InsertResultTemplate service="SOS" version="2.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:swes="http://www.opengis.net/swes/2.0"
    xmlns:sos="http://www.opengis.net/sos/2.0"
    xmlns:swe="http://www.opengis.net/swe/2.0"
    xmlns:sml="http://www.opengis.net/sensorML/1.0.1"
    xmlns:gml="http://www.opengis.net/gml/3.2"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:om="http://www.opengis.net/om/2.0"
    xmlns:sams="http://www.opengis.net/samplingSpatial/2.0"
    xmlns:sf="http://www.opengis.net/sampling/2.0"
    xmlns:xs="http://www.w3.org/2001/XMLSchema" xsi:schemaLocation="http://www.opengis.net/sos/2.0 http://schemas.opengis.net/sos/2.0/sosInsertResultTemplate.xsd http://www.opengis.net/om/2.0 http://schemas.opengis.net/om/2.0/observation.xsd  http://www.opengis.net/samplingSpatial/2.0 http://schemas.opengis.net/samplingSpatial/2.0/spatialSamplingFeature.xsd">
      <sos:proposedTemplate>
        <!-- Before using this example, make sure that all preconditions are fulfilled, 
        e.g. perform InsertSensor example. -->
        <sos:ResultTemplate>
          <swes:identifier>@@system wide unique template ID@@</swes:identifier>
          <sos:offering>@@offering defined in sensor XML@@</sos:offering>
          <sos:observationTemplate>
            <om:OM_Observation gml:id="@@result template xml document id@@">
            <om:type xlink:href="http://www.opengis.net/def/observationType/OGC-OM/2.0/OM_Measurement"/>
            <om:phenomenonTime nilReason="template"/>
            <om:resultTime nilReason="template"/>
            <om:procedure xlink:href="@@link to sensor unique id@@"/>
            <om:observedProperty xlink:href="@@link to sensor observed property@@"/>

            <om:featureOfInterest>
              <sams:SF_SpatialSamplingFeature gml:id="@@feature xml document id@@">
                <gml:identifier codeSpace="">@@point feature uri@@</gml:identifier>
                <gml:name>@@text point feature name@@</gml:name>
                <sf:type xlink:href="http://www.opengis.net/def/samplingFeatureType/OGC-OM/2.0/SF_SamplingPoint"/>
                <sf:sampledFeature xlink:href="@@domain feature uri@@"/>
                <sams:shape>
                  <gml:Point gml:id="@@feature point xml document id@@">
                    <gml:pos srsName="http://www.opengis.net/def/crs/EPSG/0/4326">@@lat-lon-values@@</gml:pos>
                  </gml:Point>
                </sams:shape>
              </sams:SF_SpatialSamplingFeature>
            </om:featureOfInterest>

            <om:result/>
          </om:OM_Observation>
        </sos:observationTemplate>
        <sos:resultStructure>
          <swe:DataRecord>
            <swe:field name="phenomenonTime">
              <swe:Time definition="http://www.opengis.net/def/property/OGC/0/PhenomenonTime">
                <swe:uom xlink:href="http://www.opengis.net/def/uom/ISO-8601/0/Gregorian"/>
              </swe:Time>
            </swe:field>
            <swe:field name="@@same-as-sensor-output@@">
              <swe:Quantity definition="@@same-as-sensor-output@@">
                <swe:uom code="@@same-as-sensor-output@@"/>
              </swe:Quantity>
            </swe:field>
          </swe:DataRecord>
        </sos:resultStructure>
        <sos:resultEncoding>
          <swe:TextEncoding tokenSeparator="#" blockSeparator="@"/>
        </sos:resultEncoding>
      </sos:ResultTemplate>
    </sos:proposedTemplate>
  </sos:InsertResultTemplate>


For data insert, a JSON object can be used like:
{
	request = "InsertResult",
        service = "SOS",
        version = "2.0.0",
	templateIdentifier = "template-unique-id",
        resultValues = "2001-01-28T12:12:01#22.4@2001-01-28T12:12:01#22.9@2001-01-28T12:15:01#23@"
}
